# Optional: If you want server-side API key storage (not recommended for BYOK)
# GEMINI_API_KEY=your_key_here
# OPENAI_API_KEY=your_key_here
# ANTHROPIC_API_KEY=your_key_here

# Optional: For legal database APIs
# LAWNET_API_KEY=your_key_here
# COURTLISTENER_API_KEY=your_key_here

# App Configuration
NEXT_PUBLIC_APP_NAME=Junas
NEXT_PUBLIC_MAX_FILE_SIZE=10485760
NEXT_PUBLIC_MAX_FILES=3

# Session Configuration (Required for secure API key storage)
# Generate a random 32+ character string for production
SESSION_SECRET=complex_password_at_least_32_characters_long_for_security

# Rate Limiting (Optional: Upstash Redis for production)
# Sign up at https://upstash.com/ for free Redis
# If not set, will use in-memory rate limiting (not recommended for production)
# UPSTASH_REDIS_REST_URL=https://your-database.upstash.io
# UPSTASH_REDIS_REST_TOKEN=your_token_here

# Monitoring (Optional: Sentry for error tracking)
# Sign up at https://sentry.io/ for free error tracking
# If not set, errors will only be logged to console
# NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
# SENTRY_AUTH_TOKEN=your_auth_token_here (only needed for source maps upload)
